<Project>

  <ItemGroup>
    <PackageVersion Include="AutoMapper.Extensions.Microsoft.DependencyInjection" Version="8.0.1" />
    <PackageVersion Include="Automatonymous" Version="5.0.1" />
    <PackageVersion Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="3.0.0" />
    <PackageVersion Include="Microsoft.NetCore.Analyzers" Version="3.0.0" />
    <PackageVersion Include="Moq" Version="4.14.5" />
    <PackageVersion Include="System.Text.Json" Version="4.7.2" />
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netcoreapp3.1' Or
                         '$(TargetFramework)' == 'netstandard2.0' Or
                         '$(TargetFramework)' == 'netstandard2.1' ">
